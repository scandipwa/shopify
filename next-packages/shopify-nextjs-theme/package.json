{
    "name": "shopify-nextjs-theme",
    "version": "0.0.1",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start"
    },
    "scandipwa": {
        "type": "theme",
        "extensions": {
            "@scandipwa/shopify-nextjs-pages": true,
            "@scandipwa/nextjs-extensibility": true
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": "@scandipwa",
        "ignorePatterns": [
            "build/**/*"
        ]
    },
    "license": "OSL-3.0",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@scandipwa/nextjs-extensibility": "0.0.0",
        "@scandipwa/shopify-nextjs-pages": "0.0.0",
        "babel-loader": "^8.2.2",
        "circular-dependency-plugin": "^5.2.2",
        "next": "10.0.5",
        "react": "17.0.1",
        "react-dom": "17.0.1"
    }
}
