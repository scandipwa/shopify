{
    "name": "shopify",
    "description": "Blank app source-code used by CSA.",
    "version": "0.0.0",
    "scripts": {
        "start": "scandipwa-scripts start",
        "build": "scandipwa-scripts build",
        "postinstall": "scandipwa-scripts link",
        "postupdate": "scandipwa-scripts link"
    },
    "dependencies": {
        "@scandipwa/framework": "0.0.3",
        "@scandipwa/router": "0.0.3",
        "@scandipwa/scandipwa-scripts": "^2.2.1",
        "@scandipwa/shopify-collections": "file:packages/@scandipwa/shopify-collections",
        "@scandipwa/shopify-api": "file:packages/@scandipwa/shopify-api",
        "@scandipwa/graphql": "file:packages/@scandipwa/graphql",
        "@scandipwa/shopify-collections-products": "file:packages/@scandipwa/shopify-collections-products",
        "@scandipwa/shopify-products": "file:packages/@scandipwa/shopify-products",
        "@scandipwa/shopify-pages": "file:packages/@scandipwa/shopify-pages"
    },
    "scandipwa": {
        "themeAlias": "Blank",
        "type": "theme",
        "extensions": {
            "@scandipwa/framework": true,
            "@scandipwa/router": true,
            "@scandipwa/shopify-collections": true,
            "@scandipwa/shopify-api": true,
            "@scandipwa/graphql": true,
            "@scandipwa/shopify-collections-products": true,
            "@scandipwa/shopify-products": true,
            "@scandipwa/shopify-pages": true
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": "@scandipwa"
    },
    "license": "OSL-3.0",
    "publishConfig": {
        "access": "public"
    }
}